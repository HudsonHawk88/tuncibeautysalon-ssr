    "scripts": {
        "test": "react scripts test",
        "copystatic": "yes | cp -rf build/public/static/images public/static/",
        "webpack:client": "webpack --env development --mode development --config webpack.client.js --watch",
        "webpack:server": "webpack --env development --mode development --config webpack.server.js --watch",
        "webpack:run": "nodemon --watch build --exec node build/bundle.js",
        "build:client": "webpack --mode production --env production --config webpack.client.js",
        "build:server": "webpack --env production --mode production --config webpack.server.js",
        "build:copy": "cp server/server.config.js build/server.config.js",
        "build": "npm run copystatic && npm-run-all --parallel build:*",
        "prod": "pm2 start build/server.config.js",
        "stop:prod": "pm2 stop tbs & pm2 delete tbs",
        "dev": "prettier --write src/ && concurrently -n frontend,server,node \"npm run webpack:client\" \"npm run webpack:server\" \"npm run webpack:run\"",
        "log": "pm2 logs"
    },
    "eslintConfig": {
